<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="20dp"
    android:orientation="vertical"
    android:paddingBottom="@dimen/dimen_part_finder_padding_top_bottom"
    android:paddingLeft="@dimen/dimen_part_finder_padding_left_right"
    android:paddingRight="@dimen/dimen_part_finder_padding_left_right"
    android:paddingTop="@dimen/dimen_part_finder_padding_top_bottom">

    <TextView
        android:id="@+id/tv_part_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="+ Add “ENGINE Oil” as New Part"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="@dimen/dimen_part_finder_title"
        tools:visibility="visible" />

</LinearLayout>